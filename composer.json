{
  "name": "thecodingmachine/weatherapi",
  "description": "This project is a demo containing a wrapper around OpenWeatherMap API. Don't use in production.",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "TheCodingMachine\\WeatherApi\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TheCodingMachine\\WeatherApi\\" : "tests/"
    }
  },
  "require": {
    "php-http/client-implementation": "^1.0",
    "psr/log": "^1.0",
    "psr/cache": "^1.0",
    "php-http/message": "^1.4",
    "guzzlehttp/psr7": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "guzzlehttp/guzzle": "^6.0",
    "php-http/guzzle6-adapter": "^1.1",
    "monolog/monolog": "^1.9",
    "tedivm/stash": "^0.14"
  }
}
